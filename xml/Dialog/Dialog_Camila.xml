<?xml version="1.0" encoding="UTF-8"?>
<idspace id="Dialog">
	<class name="Camila_Main">
		<Scripts>
			<property Type="Script" Script="ProgressPreDialogCheck_CommonNPC"/>
		</Scripts>
	</class>
	<class name="Camila_Selection">
		<Scripts>
			<property Type="Switch" C_DlgName="npc.Info.Title" C_TestTarget="company.Npc[npc.name].QP">
				<property Case="0">
					<property Type="Jump" JumpTo="Camila_QP_0"/>
				</property>
				<property Case="1">
					<property Type="Jump" JumpTo="Camila_QP_1"/>
				</property>
				<property Case="2">
					<property Type="Jump" JumpTo="Camila_QP_2"/>
				</property>
				<property Case="3">
					<property Type="Jump" JumpTo="Camila_QP_3"/>
				</property>
				<property Case="4">
					<property Type="Jump" JumpTo="Camila_QP_4"/>
				</property>
				<property Case="5">
					<property Type="Jump" JumpTo="Camila_QP_5"/>
				</property>
				<property Case="6">
					<property Type="Jump" JumpTo="Camila_QP_6"/>
				</property>
				<property Case="7">
					<property Type="Jump" JumpTo="Camila_QP_7"/>
				</property>
				<property Case="8">
					<property Type="Jump" JumpTo="Camila_QP_8"/>
				</property>
				<property Case="9">
					<property Type="Jump" JumpTo="Camila_QP_9"/>
				</property>
				<property Case="10">
					<property Type="Jump" JumpTo="Camila_QP_10"/>
				</property>
			</property>
		</Scripts>
	</class>
	<class name="Camila_QP_0">
		<Scripts>
			<property Type="Dialog" C_DlgName="npc.Info.Title">
				<property Text="안녕하세요. $CompanyName$" C_CompanyName="company.CompanyName"/>
				<property Text="저는 이능석 거래를 중개하는 까밀라입니다."/>
			</property>
			<property Type="Action" Command="UpdateCompanyProperty" PropertyType="Npc/Camila/QP" C_PropertyValue="1"/>
		</Scripts>
	</class>
	<class name="Camila_QP_1">
		<Scripts>
			<property Type="Dialog" C_DlgName="npc.Info.Title">
				<property Text="안녕하세요. $CompanyName$" C_CompanyName="company.CompanyName"/>
				<property Text="까밀라는 $CompanyName$의 성공과 번영을 위해 노력합니다. " C_CompanyName="company.CompanyName"/>
			</property>
		</Scripts>
	</class>
</idspace>
