<?xml version="1.0" encoding="UTF-8"?>
<idspace id="Dialog">
	<class name="Dimitri_Main">
		<Scripts>
			<property Type="Script" Script="ProgressPreDialogCheck_CommonNPC"/>
		</Scripts>
	</class>
	<class name="Dimitri_Selection">
		<Scripts>
			<property Type="Switch" C_DlgName="npc.Info.Title" C_TestTarget="math.random(1,2)">
				<property Case="1">
					<property Type="Dialog" C_DlgName="npc.Info.Title">
						<property Text="모든 건 고유의 가치를 지니지."/>
					</property>
				</property>
				<property Case="2">
					<property Type="Dialog" C_DlgName="npc.Info.Title">
						<property Text="가치 있는 물건이 있다면 가져 오게나."/>
					</property>
				</property>
			</property>
		</Scripts>
	</class>
</idspace>
