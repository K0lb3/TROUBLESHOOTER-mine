<?xml version="1.0" encoding="UTF-8"?>
<idspaces>
	<idspace id="SystemMenuType">
		<class name="Tutorial">
			<Buttons>
				<property Type="Resume"/>
				<property Type="MissionObjectives"/>
				<property Type="KeyBinding"/>
				<property Type="Help"/>
				<property Type="Option"/>
				<property Type="Logout_Mission"/>
				<property Type="Exit"/>
			</Buttons>
		</class>
		<class name="Tutorial_Operator">
			<Buttons>
				<property Type="Resume"/>
				<property Type="MissionObjectives"/>
				<property Type="KeyBinding"/>
				<property Type="Help"/>
				<property Type="Option"/>
				<property Type="QuitMission"/>
				<property Type="Logout_Mission"/>
				<property Type="Exit"/>
			</Buttons>
		</class>
		<class name="Lobby">
			<Buttons>
				<property Type="Resume"/>
				<property Type="DifficultySelect"/>
				<property Type="KeyBinding"/>
				<property Type="Help"/>
				<property Type="Collection"/>
				<property Type="Option"/>
				<property Type="Logout"/>
				<property Type="Exit"/>
			</Buttons>
		</class>
		<class name="Office">
			<Buttons>
				<property Type="Resume"/>
				<property Type="DifficultySelect"/>
				<property Type="KeyBinding"/>
				<property Type="Help"/>
				<property Type="Collection"/>
				<property Type="LobbyEvent"/>
				<property Type="Option"/>
				<property Type="Logout"/>
				<property Type="Exit"/>
			</Buttons>
		</class>
		<class name="Mission">
			<Buttons>
				<property Type="Resume"/>
				<property Type="MissionObjectives"/>
				<property Type="KeyBinding"/>
				<property Type="Help"/>
				<property Type="Option"/>
				<property Type="QuitMission"/>
				<property Type="Logout_Mission"/>
				<property Type="Exit_Mission"/>
			</Buttons>
		</class>
	</idspace>
	<idspace id="SystemMenu">
		<schema>
			<rule property="Script" type="function" default="None"/>
		</schema>
		<class name="Resume" Title="게임 계속하기" Script="ResumeSystemMenu"/>
		<class name="Option" Title="환경 설정" Script="ShowOption"/>
		<class name="DifficultySelect" Title="난이도 설정" Script="ShowGameDifficulty"/>
		<class name="KeyBinding" Title="단축키 설정" Script="ShowGameKeyBinding"/>
		<class name="QuitMission" Title="임무 포기" Script="QuitMission"/>
		<class name="Logout_Mission" Title="메인 메뉴로 가기" Script="Logout_Mission"/>
		<class name="Logout" Title="메인 메뉴로 가기" Script="Logout"/>
		<class name="Exit" Title="바탕 화면으로 나가기" Script="ExitGame"/>
		<class name="Exit_Mission" Title="바탕 화면으로 나가기" Script="ExitGame_Mission"/>
		<class name="MissionObjectives" Title="승리 조건" Script="ShowMissionObjectives"/>
		<class name="Help" Title="도움말" Script="ShowGameHelp"/>
		<class name="Collection" Title="갤러리" Script="ShowGameCollection"/>
		<class name="LobbyEvent" Title="로비 이벤트" Script="ShowGameLobbyEvent"/>
	</idspace>
</idspaces>
